<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.hour.schoolretail.mapper.GithubUserMapper">

    <resultMap id="BaseResultMap" type="edu.hour.schoolretail.entity.GithubUser">
            <id property="id" column="github_id" jdbcType="INTEGER"/>
            <result property="nickname" column="github_nickname" jdbcType="VARCHAR"/>
            <result property="image" column="github_image" jdbcType="VARCHAR"/>
            <result property="email" column="github_email" jdbcType="VARCHAR"/>
            <result property="signature" column="github_signature" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        github_id,github_nickname,github_image,
        github_email,github_signature
    </sql>
</mapper>
